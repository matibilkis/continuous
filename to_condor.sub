executable=subash_train.sh

output = ../jobs/outs/$(Item)_$(itraj).out
error = ../jobs/errs/$(Item)_$(itraj).err
log = ../jobs/logs/$(Item)_$(itraj).log
request_cpus = 1
request_memory = 2 GB
requirements= Has_avx == true
+flavour="medium"

itraj=0
ppp=500
periods=40

args=$(itraj) $(periods) $(Item) $(ppp)
queue from seq 1 1 8|



