executable=subash.sh

output = ../jobs/outs/$(Item).out
error = ../jobs/errs/$(Item).err
log = ../jobs/logs/$(Item).log
request_cpus = 1
+flavour="espresso"

L=1
ch=0.01
args=$(Item) $(L) $(ch)
queue from seq 10 20 10|

L=2
ch=0.01
args=$(Item) $(L) $(ch)
queue from seq 0.01 0.05 3.0|

L=3
ch=0.01
args=$(Item) $(L) $(ch)
queue from seq 0.01 0.05 3.0|
